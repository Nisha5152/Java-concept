import java.util.Scanner;

public class Problem2_2 {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int a=sc.nextInt();
		if(a>=85)
		{
			System.out.println("A");
		}
		else if(a>=75)
		{
			System.out.println("B");
		}
		else if(a>=65)
		{
			System.out.println("C");
		}
		else
		{
			System.out.println("D");
		}
		

	}

}
